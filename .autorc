{
  "plugins": [
    "npm",
    "released",
    "first-time-contributor",
    "all-contributors"
  ],
  "owner": "chara-codes",
  "repo": "chara",
  "name": "Chara Team",
  "email": "team@chara.codes",
  "labels": [
    {
      "name": "💥 Breaking Change",
      "changelogTitle": "💥 Breaking Change",
      "description": "A change that breaks existing functionality",
      "releaseType": "major",
      "color": "d73a4a"
    },
    {
      "name": "🚀 Feature",
      "changelogTitle": "🚀 Enhancement",
      "description": "New feature or enhancement",
      "releaseType": "minor",
      "color": "a2eeef"
    },
    {
      "name": "🐛 Bug Fix",
      "changelogTitle": "🐛 Bug Fix",
      "description": "A bug fix",
      "releaseType": "patch",
      "color": "d73a4a"
    },
    {
      "name": "📚 Documentation",
      "changelogTitle": "📚 Documentation",
      "description": "Documentation changes",
      "releaseType": "patch",
      "color": "0075ca"
    },
    {
      "name": "🏠 Internal",
      "changelogTitle": "🏠 Internal",
      "description": "Internal changes",
      "releaseType": "patch",
      "color": "ffffff"
    },
    {
      "name": "🏃 Performance",
      "changelogTitle": "🏃 Performance",
      "description": "Performance improvements",
      "releaseType": "patch",
      "color": "0e8a16"
    },
    {
      "name": "📦 Dependencies",
      "changelogTitle": "📦 Dependencies",
      "description": "Dependency updates",
      "releaseType": "patch",
      "color": "0366d6"
    },
    {
      "name": "🚫 Skip Release",
      "description": "Skip releasing this PR",
      "releaseType": "skip",
      "color": "6f42c1"
    }
  ],
  "baseBranch": "main",
  "prereleaseBranches": ["playground"],
  "author": {
    "name": "Chara Team",
    "email": "team@chara.codes"
  },
  "noVersionPrefix": false,
  "onlyPublishWithReleaseLabel": false,
  "shipit": {
    "onlyGraduateWithReleaseLabel": false,
    "message": "Update version to: v%v [skip ci]"
  },
  "changelog": {
    "message": "Update CHANGELOG.md [skip ci]"
  }
}
