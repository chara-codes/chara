{
  "name": "@chara-codes/tunnel",
  "version": "0.1.17",
  "description": "A lightweight tunneling service that exposes local development servers to the internet through custom subdomains",
  "keywords": [
    "tunnel",
    "ngrok",
    "localtunnel",
    "chara",
    "development",
    "proxy",
    "websocket",
    "subdomain"
  ],
  "author": "Chara Team",
  "module": "./src/index.ts",
  "type": "module",
  "bin": {
    "tunnel": "./dist/tunnel"
  },
  "scripts": {
    "bbuild": "bun build ./src/cli.ts --outfile ./dist/tunnel --compile",
    "d": "bun ./src/cli.ts",
    "start": "./dist/tunnel"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.8.2"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/chara-codes/chara.git",
    "directory": "packages/tunnel"
  },
  "dependencies": {
    "@chara-codes/logger": "workspace:*",
    "eventemitter3": "^5.0.1",
    "human-id": "^4.1.1",
    "picocolors": "^1.1.1",
    "yargs": "^17.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
